@prefix ocp: <http://www.semanticweb.org/DigitalizeMe/ontologies/2025/3/OCP#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/FunctionalQualityCheck> a time:Instant ;
    ocp:belongsToCycle <http://example.org/QualityCheckCycle> ;
    ocp:belongsToPhase <http://example.org/QualityManagementPhase> ;
    ocp:hasActualTime "2025-08-28T00:00:00+00:00"^^xsd:dateTime ;
    ocp:hasEstimatedTime "2025-08-28T00:00:00+00:00"^^xsd:dateTime .

<http://example.org/FunctionalQualityCheck2> a time:Instant ;
    ocp:belongsToCycle <http://example.org/QualityCheckCycle2> ;
    ocp:belongsToPhase <http://example.org/QualityManagementPhase> ;
    ocp:hasActualTime "2025-09-06T00:00:00+00:00"^^xsd:dateTime ;
    ocp:hasEstimatedTime "2025-09-07T00:00:00+00:00"^^xsd:dateTime .

<http://example.org/PlanningTransition> a ocp:Transition,
        time:Instant,
        time:TemporalEntity ;
    ocp:belongsToProcess <http://example.org/ProductSellingProcess> ;
    ocp:hasActualTime "2025-08-16T00:00:00+00:00"^^xsd:dateTime ;
    time:after <http://example.org/PlanningEnd> .

<http://example.org/QualityCheckTransition> a ocp:Transition,
        time:Instant,
        time:TemporalEntity ;
    ocp:belongsToProcess <http://example.org/ProductSellingProcess> ;
    ocp:hasActualTime "2025-09-08T00:00:00+00:00"^^xsd:dateTime ;
    ocp:hasEstimatedTime "2025-09-12T00:00:00+00:00"^^xsd:dateTime ;
    time:after <http://example.org/QualityCheckApproval> .

<http://example.org/ShippingTransition> a ocp:Transition,
        time:Instant,
        time:TemporalEntity ;
    ocp:belongsToProcess <http://example.org/ProductSellingProcess> ;
    ocp:hasActualTime "2025-09-15T00:00:00+00:00"^^xsd:dateTime ;
    time:after <http://example.org/ShippingEnd> .

<http://example.org/VisualQualityCheck> a time:Instant ;
    ocp:belongsToCycle <http://example.org/QualityCheckCycle> ;
    ocp:belongsToPhase <http://example.org/QualityManagementPhase> ;
    ocp:hasActualTime "2025-08-27T00:00:00+00:00"^^xsd:dateTime ;
    ocp:hasEstimatedTime "2025-08-27T00:00:00+00:00"^^xsd:dateTime .

<http://example.org/VisualQualityCheck2> a time:Instant ;
    ocp:belongsToCycle <http://example.org/QualityCheckCycle2> ;
    ocp:belongsToPhase <http://example.org/QualityManagementPhase> ;
    ocp:hasActualTime "2025-09-05T00:00:00+00:00"^^xsd:dateTime ;
    ocp:hasEstimatedTime "2025-09-06T00:00:00+00:00"^^xsd:dateTime .

<http://example.org/CustomerManagementPhase> a ocp:Phase,
        time:Interval,
        time:TemporalEntity .

<http://example.org/ShippingStart> a time:Instant ;
    ocp:hasActualTime "2025-09-08T00:00:00+00:00"^^xsd:dateTime ;
    ocp:startsPhase <http://example.org/ShippingPhase> .

<http://example.org/MarketingEnd> a time:Instant ;
    ocp:endsPhase <http://example.org/MarketingPhase> ;
    ocp:hasActualTime "2025-09-10T00:00:00+00:00"^^xsd:dateTime ;
    ocp:hasEstimatedTime "2025-09-12T00:00:00+00:00"^^xsd:dateTime .

<http://example.org/MarketingStart> a time:Instant ;
    ocp:hasActualTime "2025-08-22T00:00:00+00:00"^^xsd:dateTime ;
    ocp:hasEstimatedTime "2025-08-20T00:00:00+00:00"^^xsd:dateTime ;
    ocp:startsPhase <http://example.org/MarketingPhase> .

<http://example.org/PlanningEnd> a time:Instant ;
    ocp:endsPhase <http://example.org/PlanningPhase> ;
    ocp:hasActualTime "2025-08-15T00:00:00+00:00"^^xsd:dateTime .

<http://example.org/PlanningPhase> a ocp:Phase,
        time:Interval,
        time:TemporalEntity ;
    ocp:hasActualEnd <http://example.org/PlanningEnd> .

<http://example.org/ProductionEnd> a time:Instant ;
    ocp:endsPhase <http://example.org/ProductionPhase> ;
    ocp:hasActualTime "2025-08-25T00:00:00+00:00"^^xsd:dateTime ;
    ocp:hasEstimatedTime "2025-08-23T00:00:00+00:00"^^xsd:dateTime .

<http://example.org/ProductionStart> a time:Instant ;
    ocp:hasActualTime "2025-08-22T00:00:00+00:00"^^xsd:dateTime ;
    ocp:hasEstimatedTime "2025-08-20T00:00:00+00:00"^^xsd:dateTime ;
    ocp:startsPhase <http://example.org/ProductionPhase> .

<http://example.org/QualityCheckRejection> a time:Instant ;
    ocp:belongsToPhase <http://example.org/QualityManagementPhase> ;
    ocp:endsCycle <http://example.org/QualityCheckCycle> ;
    ocp:hasActualTime "2025-08-29T00:00:00+00:00"^^xsd:dateTime ;
    ocp:hasEstimatedTime "2025-08-30T00:00:00+00:00"^^xsd:dateTime .

<http://example.org/QualityCheckStart2> a time:Instant ;
    ocp:belongsToPhase <http://example.org/QualityManagementPhase> ;
    ocp:hasActualTime "2025-09-04T00:00:00+00:00"^^xsd:dateTime ;
    ocp:hasEstimatedTime "2025-09-05T00:00:00+00:00"^^xsd:dateTime ;
    ocp:startsCycle <http://example.org/QualityCheckCycle2> .

<http://example.org/ShippingEnd> a time:Instant ;
    ocp:endsPhase <http://example.org/ShippingPhase> ;
    ocp:hasActualTime "2025-09-14T00:00:00+00:00"^^xsd:dateTime .

<http://example.org/MarketingPhase> a ocp:Phase,
        time:Interval,
        time:TemporalEntity ;
    ocp:beginsWith <http://example.org/ProductionPhase> ;
    ocp:endsAfter <http://example.org/QualityManagementPhase> ;
    ocp:hasActualBeginning <http://example.org/MarketingStart> ;
    ocp:hasActualEnd <http://example.org/MarketingEnd> ;
    ocp:hasEstimatedBeginning <http://example.org/MarketingStart> ;
    ocp:hasEstimatedEnd <http://example.org/MarketingEnd> .

<http://example.org/ShippingPhase> a ocp:Phase,
        time:Interval,
        time:TemporalEntity ;
    ocp:hasActualBeginning <http://example.org/ShippingStart> ;
    ocp:hasActualEnd <http://example.org/ShippingEnd> .

<http://example.org/QualityCheckCycle> a ocp:Cycle,
        time:Interval,
        time:TemporalEntity ;
    ocp:belongsToProcess <http://example.org/ProductSellingProcess> ;
    ocp:hasActualBeginning <http://example.org/QualityCheckStart> ;
    ocp:hasActualEnd <http://example.org/QualityCheckRejection> ;
    ocp:hasCycleNumber 1 ;
    ocp:hasEstimatedBeginning <http://example.org/QualityCheckStart> ;
    ocp:hasEstimatedEnd <http://example.org/QualityCheckRejection> .

<http://example.org/QualityCheckCycle2> a ocp:Cycle,
        time:Interval,
        time:TemporalEntity ;
    ocp:belongsToProcess <http://example.org/ProductSellingProcess> ;
    ocp:hasActualBeginning <http://example.org/QualityCheckStart2> ;
    ocp:hasActualEnd <http://example.org/QualityCheckApproval> ;
    ocp:hasCycleNumber 2 ;
    ocp:hasEstimatedBeginning <http://example.org/QualityCheckStart2> ;
    ocp:hasEstimatedEnd <http://example.org/QualityCheckApproval> .

<http://example.org/QualityCheckStart> a time:Instant ;
    ocp:hasActualTime "2025-08-26T00:00:00+00:00"^^xsd:dateTime ;
    ocp:hasEstimatedTime "2025-08-26T00:00:00+00:00"^^xsd:dateTime ;
    ocp:startsCycle <http://example.org/QualityCheckCycle> ;
    ocp:startsPhase <http://example.org/QualityManagementPhase> .

<http://example.org/ProductSellingProcess> a ocp:Process,
        time:Interval,
        time:TemporalEntity ;
    ocp:containsPhase <http://example.org/CustomerManagementPhase>,
        <http://example.org/MarketingPhase>,
        <http://example.org/PlanningPhase>,
        <http://example.org/ProductionPhase>,
        <http://example.org/QualityManagementPhase>,
        <http://example.org/ShippingPhase> ;
    ocp:hasProcessID "PSP_001"^^xsd:string .

<http://example.org/ProductionPhase> a ocp:Phase,
        time:Interval,
        time:TemporalEntity ;
    ocp:hasActualBeginning <http://example.org/ProductionStart> ;
    ocp:hasActualEnd <http://example.org/ProductionEnd> ;
    ocp:hasEstimatedBeginning <http://example.org/ProductionStart> ;
    ocp:hasEstimatedEnd <http://example.org/ProductionEnd> .

<http://example.org/QualityCheckApproval> a time:Instant ;
    ocp:endsCycle <http://example.org/QualityCheckCycle2> ;
    ocp:endsPhase <http://example.org/QualityManagementPhase> ;
    ocp:hasActualTime "2025-09-07T00:00:00+00:00"^^xsd:dateTime ;
    ocp:hasEstimatedTime "2025-09-10T00:00:00+00:00"^^xsd:dateTime .

<http://example.org/QualityManagementPhase> a ocp:Phase,
        time:Interval,
        time:TemporalEntity ;
    ocp:beginsAfter <http://example.org/ProductionPhase> ;
    ocp:hasActualBeginning <http://example.org/QualityCheckStart> ;
    ocp:hasActualEnd <http://example.org/QualityCheckApproval> ;
    ocp:hasEstimatedBeginning <http://example.org/QualityCheckStart> ;
    ocp:hasEstimatedEnd <http://example.org/QualityCheckApproval> .

