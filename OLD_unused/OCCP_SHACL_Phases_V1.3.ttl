    osh:ShapeOfPhaseL a sh:NodeShape ;              # Phase L (Repair)
        sh:targetClass occp:PhaseL_Repair ;
        # Ensure PhaseL ends with or after PhaseK_Damage based on actual or estimated times
        sh:sparql [
