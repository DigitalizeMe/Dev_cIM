    osh:ShapeOfPhaseM a sh:NodeShape ;              # Phase M (Deconstruction)
        sh:targetClass occp:PhaseM_Deconstruction ;
        # Ensure PhaseM occurs after CompletionOfConstruction based on actual or estimated times
        sh:sparql [
