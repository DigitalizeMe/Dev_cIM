PREFIX ex:   <http://www.example.de/example#>
PREFIX ocp:  <http://www.semanticweb.org/DigitalizeMe/ontologies/2025/3/OCP#>
PREFIX osh:  <http://www.ocpshape.de/shape#>
PREFIX rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX sh:   <http://www.w3.org/ns/shacl#>
PREFIX time: <http://www.w3.org/2006/time#>
PREFIX xsd:  <http://www.w3.org/2001/XMLSchema#>

[ rdf:type     sh:ValidationReport;
  sh:conforms  false;
  sh:result    [ rdf:type                      sh:ValidationResult;
                 sh:focusNode                  ex:P2;
                 sh:resultMessage              "For temporal entity <http://www.example.de/example#P2>: Earliest start time (2025-07-01 from instant <http://www.example.de/example#Instant3>) is not after latest end time (2026-06-30 from instant <http://www.example.de/example#Instant6>) of entity <http://www.example.de/example#P3> as required by time:after.";
                 sh:resultSeverity             sh:Violation;
                 sh:sourceConstraintComponent  sh:SPARQLConstraintComponent;
                 sh:sourceShape                osh:AfterTimeShape;
                 sh:value                      ex:P2
               ];
  sh:result    [ rdf:type                      sh:ValidationResult;
                 sh:focusNode                  ex:P2;
                 sh:resultMessage              "For temporal entity <http://www.example.de/example#P2>: Latest end time (2025-12-31 from instant <http://www.example.de/example#Instant4>) is not before earliest start time (2025-12-01 from instant <http://www.example.de/example#Instant7>) of entity <http://www.example.de/example#P4> as required by time:before.";
                 sh:resultSeverity             sh:Violation;
                 sh:sourceConstraintComponent  sh:SPARQLConstraintComponent;
                 sh:sourceShape                osh:BeforeTimeShape;
                 sh:value                      ex:P2
               ]
] .
